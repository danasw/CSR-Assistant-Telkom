<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="jogja.telkom.csrassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddOn.AddOnActivity"/>
        <activity android:name=".AddOn.TambahAddOnActivity"/>
        <activity android:name=".AddOn.CabutAddOnActivity"/>
        <activity android:name=".Upgrade.UpgradeActivity" />
        <activity android:name=".Upgrade.SatuPaketUpgradeActivity" />
        <activity android:name=".Upgrade.BedaPaketUpgradeActivity" />
        <activity android:name=".Kendala.KendalaActivity" />
        <activity android:name=".Isolir.IsolirActivity" />
        <activity android:name=".Tagihan.TagihanActivity" />
        <activity android:name=".Tagihan.TetapDitagihTagihanActivity" />
        <activity android:name=".Tagihan.TidakSesuaiTagihanActivity" />
    </application>

</manifest>